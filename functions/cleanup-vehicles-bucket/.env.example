# Appwrite base
APPWRITE_ENDPOINT=https://appwrite.racoondevs.com/v1
APPWRITE_PROJECT_ID=YOUR_PROJECT_ID
APPWRITE_API_KEY=YOUR_API_KEY

# Targets
APPWRITE_DATABASE_ID=YOUR_DATABASE_ID
COLLECTION_VEHICLE_FILES_ID=YOUR_VEHICLE_FILES_COLLECTION_ID
BUCKET_VEHICLES_ID=YOUR_VEHICLES_BUCKET_ID

# TTL + execution
ORPHAN_TTL_HOURS=24
DRY_RUN=true

# Safety
PAGE_SIZE=100
MAX_FILES_PER_RUN=2000

# Link rules
ONLY_ENABLED_LINKS=true
FILE_ID_ATTRIBUTE=fileId
ENABLED_ATTRIBUTE=enabled
